@model System.Collections.Generic.IEnumerable<Pagila.ViewModel.CustomerViewModel>

@{
}

<h2>Liste</h2>

<p>
    @Html.ActionLink("Ekle", "Create")
</p>
<table class="table">
   <tr>
       <th>
           @Html.DisplayNameFor(model => model.StoreId)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.FirstName)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.LastName)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.Email)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.AddressId)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.Activebool)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.CreateDate)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.LastUpdate)
       </th>
       <th>
           @Html.DisplayNameFor(model => model.Active)
       </th>
       <th>
           @Html.Label("", "Ýþlemler")
       </th>
   </tr>
   @foreach (var item in Model)
   {
       <tr>
           <td>
               @(item.StoreId)
           </td>
           <td>
               @(item.FirstName)
           </td>
           <td>
               @(item.LastName)
           </td>
           <td>
               @(item.Email)
           </td>
           <td>
               @(item.AddressId)
           </td>
           <td>
               @(item.Activebool)
           </td>
           <td>
               @(item.CreateDate)
           </td>
           <td>
               @(item.LastUpdate)
           </td>
           <td>
               @(item.Active)
           </td>
           <td>
               @Html.ActionLink("Detay", "Detail", new { customerId = item.CustomerId }) |
               @Html.ActionLink("Düzenle", "Edit", new { customerId = item.CustomerId }) |
               @Html.ActionLink("Sil", "Delete", new { customerId = item.CustomerId })
           </td>
       </tr>
   }
</table>
<div>
       <ul>
           <li>@Html.ActionLink("Anasayfaya dön", "Index", "Home")</li>
       </ul>
</div>